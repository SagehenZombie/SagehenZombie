
<canvas id="canvas"></canvas>
<script src="javascript/jquery.js"></script>
<script src="javascript/easeljs-0.8.0.min.js"></script>
<script type="text/javascript" src="javascript/player.js"></script>
<script type="text/javascript" src="javascript/polyk.js"></script>
<script type="text/javascript" src="javascript/paper-full.js"></script>
<script type="text/javascript" src="javascript/background.js"></script>
<script type="text/javascript" src="javascript/polygon.js"></script>
<script type="text/javascript">
    // Only executed our code once the DOM is ready.
    window.onload = function() {


        var stage = new createjs.Stage("canvas");
        var sagehen_front_data = new createjs.SpriteSheet({
            images: ["images/sagehen_front.png","images/sagehen_front_walk_1.png","images/sagehen_front_walk_2.png"],
            frames: { width:32, height:32 },
            animations: {
                walk: [0,2,"walk"]
            }
        });

        var sagehen_front_spriteSheet = new createjs.SpriteSheet(sagehen_front_data);
        var sagehen_front_walk_animation = new createjs.Sprite(sagehen_front_spriteSheet, 0);
        sagehen_front_walk_animation.x = 500;
        sagehen_front_walk_animation.y = 500;
        sagehen_front_walk_animation.gotoAndPlay("walk");
        stage.addChild(sagehen_front_walk_animation);
        stage.update();
    }
</script>